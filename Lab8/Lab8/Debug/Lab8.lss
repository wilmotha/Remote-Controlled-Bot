                                 ;*	Program Initialization
                                 ;***********************************************************
                                 INIT:
                                 	;Stack Pointer (VERY IMPORTANT!!!!)
000046 e100                      	ldi mpr, high(RAMEND)	
000047 bf0e                      	out sph, mpr
000048 ef0f                      	ldi mpr, low(RAMEND)
000049 bf0d                      	out spl, mpr
                                 
                                 	;I/O Ports
00004a e000                      	ldi mpr, (0<<PD0)|(0<<PD1)|(0<<PD2)	;This sets up the buttons for input for the two interrupts and PD2 is for the Recieve interrupt for USART. 
00004b bb01                      	out DDRD, mpr	;the value is loaded into DDRD to set the port as input
                                 
00004c e003                      	ldi mpr, $03	; This set the port for input
00004d bb02                      	out PORTD, mpr
                                 
00004e ef0f                      	ldi mpr, $FF	;Sets DDRB as output
00004f bb07                      	out DDRB, mpr
                                 
000050 e000                      	ldi mpr, $00	;sets Port b as output which lets the led be turned on
000051 bb08                      	out PortB, mpr
                                 	
000052 e060                      	ldi Fcount, 0
                                 
                                 	;USART1
                                 		;Set baudrate at 2400bps
000053 e003                      	ldi mpr, high(832)	;set the baud rate using the double baud rate equation
000054 9300 0098                 	sts UBRR1H, mpr
000056 e400                      	ldi mpr, low(832)
000057 9300 0099                 	sts UBRR1L, mpr
                                 	
000059 e002                      	ldi mpr, (1<<U2X1)	;sets the baud rate bit 
00005a 9300 009b                 	sts UCSR1A, mpr
                                 
00005c e908                      	ldi mpr, (1<<RXCIE1)|(1<<RXEN1)|(1<<TXEN1)	;sets the Receive enable and transmit enable and receive interrupt enable
00005d 9300 009a                 	sts UCSR1B, mpr
                                 
00005f e00e                      	ldi mpr, (1<<USBS1)|(1<<UCSZ11)|(1<<UCSZ10)	;sets the 8 bit rate and 2 stop bits
000060 9300 009d                 	sts UCSR1C, mpr
                                 	
                                 	;External Interrupts
                                 		;Set the External Interrupt Mask
000062 e003                      	ldi mpr, 0b00000011		;set up the two buttons as inputs or as bumpers
000063 bf09                      	out EIMSK, mpr
                                 		;Set the Interrupt Sense Control to falling edge detection
000064 e00a                      	ldi mpr, 0b00001010		;set up the two buttons interrupts as falling edge detection
000065 9300 006a                 	sts EICRA, mpr
                                 
000067 9478                      	sei
                                 	;Other
000068 e0a0                      	ldi XL, low(BUFFER)		;set up X and Y to point to the buffer which stores the value that is received
000069 e0b1                      	ldi XH, high(BUFFER)
00006a d09e                      	rcall Resetx			;resets the value that x pointing to
                                 
                                 	
