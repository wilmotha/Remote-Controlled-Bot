000000 c045                      		rjmp 	INIT			; Reset interrupt
                                 
                                 ;Should have Interrupt vectors for:
                                 ;- Left whisker
                                 .org	$0002
000002 c0c9                      		rjmp	LeftBump
                                 ;- Right whisker
                                 .org	$0004
000004 c0a8                      		rjmp	RightBump
                                 ;- USART receive
                                 .org	$003C
00003c c0ae                      		rjmp	Receive
                                 		; some subroutine?
                                 .org	$0046					; End of Interrupt Vectors
                                 		;rjmp	Receive
